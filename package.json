{
  "name": "audiol",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:src": "eslint src",
    "lint:scripts": "eslint scripts",
    "preview": "vite preview",
    "sync:products": "tsx scripts/sync-to-supabase.ts",
    "sync:stores": "tsx scripts/sync-stores.ts && tsx scripts/process-store-products.ts",
    "sync:stores:iem": "tsx scripts/sync-stores-iem.ts && tsx scripts/process-store-products-iem.ts",
    "sync:stores:headphone": "tsx scripts/sync-stores-headphone.ts && tsx scripts/process-store-products-headphone.ts",
    "sync:prices": "tsx scripts/bestbuy-sync.ts --mode=daily && tsx scripts/process-store-products-iem.ts && tsx scripts/process-store-products-headphone.ts && tsx scripts/process-store-products-microphone.ts",
    "bestbuy:taxonomy": "tsx scripts/bestbuy-taxonomy.ts",
    "sync:amazon": "tsx scripts/amazon-bulk-sync.ts --mode=daily --time-budget=300 --concurrency=6 --delay=3000",
    "sync:aliexpress": "tsx scripts/aliexpress-sync.ts --mode=daily --budget=4500 && tsx scripts/process-store-products.ts",
    "link:measurements": "tsx scripts/link-measurements.ts",
    "link:sinad": "tsx scripts/link-sinad.ts",
    "sync:all": "npm run sync:stores && npm run sync:stores:iem && npm run sync:stores:headphone && npm run sync:aliexpress && npm run link:measurements && npm run link:sinad && npm run sync:prices && npm run sync:amazon",
    "sync:families": "tsx scripts/parse-variants.ts",
    "admin": "tsx scripts/admin-server.ts",
    "seed:coupons": "tsx scripts/seed-coupons.ts",
    "clean:products": "tsx scripts/clean-products.ts",
    "sync:history": "tsx scripts/snapshot-prices.ts",
    "audit:bestbuy-multi-listings": "tsx scripts/audit-bestbuy-multi-listings.ts",
    "db:phase1:reset": "tsx scripts/run-phase1-reset.ts",
    "db:apply:016": "tsx scripts/run-016-catalog-products-source-id.ts",
    "db:apply:017": "tsx scripts/run-017-review-tasks-offer-and-error.ts",
    "db:apply:018": "tsx scripts/run-018-headphone-type-and-hp-accessory.ts",
    "db:apply:019": "tsx scripts/run-019-hide-discontinued-new-price-listings.ts",
    "db:apply:020": "tsx scripts/run-020-exclude-discontinued-new-from-price-rollups.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.95.2",
    "nanoid": "^5.1.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.13.0",
    "recharts": "^3.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "express": "^5.2.1",
    "globals": "^16.5.0",
    "playwright": "^1.58.2",
    "postgres": "^3.4.8",
    "supabase": "^2.76.8",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
